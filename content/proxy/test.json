{
    "proxy": ["http://127.0.0.1:8080"],
    "timeout": "10s",
    "auth": { 
        "type": "jwt",
        "source": "$query.token",
        "secret": "1234567890",
        "claims": {
            "$query.user_id": "user_id",
            "$query.user_name": "user_name"
        }
    },
    "routes": [
        {
            "match": "^/api/.*",
            "proxy": ["http://127.0.0.1:8000"],
            "disable_auth": true
        }
    ]
}